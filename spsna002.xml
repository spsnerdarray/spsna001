            <data name="http://www.3s-software.com/plcopenxml/pou" handleUnknown="implementation">
              <pou name="FB_Inverter" pouType="functionBlock">
                <interface>
                  <inputVars>
                    <variable name="enable">
                      <type>
                        <BOOL />
                      </type>
                    </variable>
                    <variable name="setPoint">
                      <type>
                        <REAL />
                      </type>
                    </variable>
                    <variable name="controlWord">
                      <type>
                        <derived name="SControlWord_t" />
                      </type>
                    </variable>
                    <variable name="rampUp">
                      <type>
                        <REAL />
                      </type>
                      <initialValue>
                        <simpleValue value="10.0" />
                      </initialValue>
                    </variable>
                    <variable name="rampDown">
                      <type>
                        <REAL />
                      </type>
                      <initialValue>
                        <simpleValue value="5.0" />
                      </initialValue>
                    </variable>
                  </inputVars>
                  <outputVars>
                    <variable name="actValue">
                      <type>
                        <REAL />
                      </type>
                    </variable>
                    <variable name="statusWord">
                      <type>
                        <derived name="SStatusWord_t" />
                      </type>
                    </variable>
                    <variable name="actPosition">
                      <type>
                        <REAL />
                      </type>
                    </variable>
                  </outputVars>
                  <localVars>
                    <variable name="rampSpeedUp">
                      <type>
                        <REAL />
                      </type>
                    </variable>
                    <variable name="rampSpeedDown">
                      <type>
                        <REAL />
                      </type>
                    </variable>
                  </localVars>
                </interface>
                <body>
                  <ST>
                    <xhtml xmlns="http://www.w3.org/1999/xhtml">


  
IF enable THEN


	
	IF rampUp &gt; 0.0 THEN
		rampSpeedUp := (GlobaleConstants.MAX_SET_POINT - GlobaleConstants.MIN_SET_POINT) / rampUp;
	ELSE
		rampSpeedUp := 999999.0;
	END_IF


	IF rampDown &gt; 0.0 THEN
		rampSpeedDown := (GlobaleConstants.MAX_SET_POINT - GlobaleConstants.MIN_SET_POINT) / rampDown;
	ELSE
		rampSpeedDown := 999999.0;
	END_IF


	setPoint := LIMIT(GlobaleConstants.MIN_SET_POINT, setPoint, GlobaleConstants.MAX_SET_POINT);

	IF controlWord.Enable THEN
	
		IF controlWord.Stop THEN
			actValue := 0.0;
		ELSIF controlWord.Start THEN
			IF setPoint &gt; actValue THEN
				actValue := actValue + rampSpeedUp * GlobaleConstants.CYCLETIME;
				IF actValue &gt; setPoint THEN
					actValue := setPoint;
				END_IF
			
			ELSIF setPoint &lt; actValue THEN
				actValue := actValue - rampSpeedDown * GlobaleConstants.CYCLETIME;
				IF actValue &lt; setPoint THEN
					actValue := setPoint;
				END_IF
			END_IF
			//actValue := setPoint;
		ELSE
			actValue := 0.0;
		END_IF
	
		IF controlWord.Direction THEN
			actValue := setPoint * -1.0;
		END_IF
		
	
	ELSE
		actValue := 0.0;
	END_IF

	statusWord.Enable := controlWord.Enable;
	statusWord.Stop := controlWord.Stop;
	statusWord.N0 := (actValue &lt;= 0.001 AND actValue &gt;= -0.001);

ELSE

	actValue := 0.0;
	statusWord.Enable := FALSE;
	statusWord.Stop := TRUE;
	statusWord.N0 := TRUE;

END_IF 

statusWord.Direction := controlWord.Direction;
</xhtml>
                  </ST>
                </body>
