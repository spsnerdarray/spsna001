                <body>
                  <ST>
                    <xhtml xmlns="http://www.w3.org/1999/xhtml">

Inverter(
	enable := GlobalVariable.Freigabe, 
	setPoint	:= GlobalVariable.Sollwert,
	controlWord := GlobalVariable.SteuerWort,
	actValue =&gt; GlobalVariable.Istwert,
	statusWord =&gt; GlobalVariable.StatusWort
);

</xhtml>
                  </ST>
                </body>
                <addData>
                  <data name="http://www.3s-software.com/plcopenxml/objectid" handleUnknown="discard">
